{
  "name": "uiowa-mfk-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build @uiowa/uiowa-mfk",
    "build:gh-pages": "ng build --base-href /uiowa-mfk-project/",
    "cypress:open": "start-server-and-test start http://localhost:4200 \"cypress open\"",
    "cypress:verify": "cypress verify",
    "cypress:run": "start-server-and-test start http://localhost:4200 \"cypress run\"",
    "watch": "ng build --watch --configuration development"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.3",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@ng-bootstrap/ng-bootstrap": "^20.0.0-rc.0",
    "@uiowa/digit-only": "^4.0.1",
    "bootstrap": "^5.3.8",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.0",
    "cypress": "^15.7.0",
    "cypress-plugin-tab": "^1.0.5",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.0",
    "start-server-and-test": "^2.1.3",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
